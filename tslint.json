{
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"linterOptions": {
		"exclude": [
			"node_modules/**/*.ts",
			"**/*.json",
			"*.json",
			"./*.json",
			"**/.lintstagedrc.json",
			"**/*.d.ts",
			"**/*.md",
			"*.md",
			"./README.md"
		]
	},
	"rules": {
		"no-console": false,
		"ordered-imports": true
	}
}
